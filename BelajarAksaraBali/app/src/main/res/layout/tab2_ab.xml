<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">


    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        app:srcCompat="@drawable/aksarawijak" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView31"
        app:srcCompat="@drawable/ekaaksara" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="220dp"
        android:layout_height="170dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/imageView31"
        android:layout_marginEnd="34dp"
        android:layout_marginRight="34dp"
        app:srcCompat="@drawable/dwiaksara" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imageView32"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:textSize="20dp"
        android:text="Eka Aksara" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView33"
        android:layout_alignLeft="@+id/imageView33"
        android:layout_alignStart="@+id/imageView33"
        android:textSize="20dp"
        android:text="Dwi Aksara" />

</RelativeLayout>